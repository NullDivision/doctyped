// @flow

<% Object.entries(properties).forEach(([,{ imports, type }]) => { -%>
<% if (imports) { %>import type { <%= type %> } from './<%- imports %>.js.flow';<% } -%>
<% }); %>

<% Object.entries(properties).forEach(([,{ exports, type }]) => { -%>
<% if (exports) { %>export type <%= type %> = <%- exports %>;<% } -%>
<% }); %>

export type <%= name %> = {
<%_ Object.entries(properties).forEach(([propName, { required, type }]) => { -%>
  <%= propName %>: <% if (!required) { %>?<% } %><%= type %>,
<% }); -%>
};
