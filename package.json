{
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "bin": {
    "doctyped": "dist/index.js"
  },
  "name": "doctyped",
  "version": "1.3.2",
  "description": "Swagger-enabled flow types generator",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:clear": "rimraf -rfv ./dist/*",
    "build:copy": "mkdirp ./dist/templates && copyfiles ./src/templates ./dist",
    "build": "npm run build:clear && npm run build:copy && tsc",
    "lint": "eslint ./src/**/*.{js,ts} ./test/**/*.{js,ts}",
    "pretest": "rimraf -rf ./tmp/*",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NullDivision/doctyped.git"
  },
  "keywords": [
    "swagger",
    "flow",
    "models"
  ],
  "author": "NullDivision",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NullDivision/doctyped/issues"
  },
  "homepage": "https://github.com/NullDivision/doctyped#readme",
  "dependencies": {
    "ejs": "^3.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.0",
    "@types/jest": "^24.0.15",
    "@types/request-promise-native": "^1.0.16",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.1.0",
    "flow-bin": "^0.104.0",
    "flow-parser": "^0.104.0",
    "jest": "^24.8.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "./tmp"
    ]
  }
}
