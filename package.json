{
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "bin": {
    "doctyped": "dist/index.js"
  },
  "name": "doctyped",
  "version": "1.3.2",
  "description": "Swagger-enabled flow types generator",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:clear": "rm -rfv ./dist/*",
    "build:copy": "mkdir -p ./dist/templates && cp -r ./src/templates ./dist",
    "build": "npm run build:clear && npm run build:copy && tsc",
    "pretest": "rm -rf ./tmp/* && npm run build",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NullDivision/doctyped.git"
  },
  "keywords": [
    "swagger",
    "flow",
    "models"
  ],
  "author": "NullDivision",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NullDivision/doctyped/issues"
  },
  "homepage": "https://github.com/NullDivision/doctyped#readme",
  "dependencies": {
    "ejs": "^2.5.8",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/ejs": "^2.6.3",
    "@types/request-promise-native": "^1.0.16",
    "@types/yargs": "^13.0.0",
    "ava": "^2.2.0",
    "flow-bin": "^0.102.0",
    "flow-parser": "^0.102.0",
    "sinon": "^7.1.1",
    "typescript": "^3.1.6"
  }
}
